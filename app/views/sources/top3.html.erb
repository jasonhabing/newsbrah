<% @bigstories2.each do |story| %>

<% unless story.feed_items.first == nil %>
<br>
<h4><%= story.feed_items.first.title %></h4>
Broke: <%= story.breakingdate.strftime("%m/%d/%Y %I:%M") %>&nbsp;&nbsp;&nbsp;Latest: <%= story.latestdate.strftime("%m/%d/%Y %I:%M") %>

<br><br>

<p>Mentioned in <%= story.feed_items.count %> sources: </p>

<% story.feed_items.each do |item| %>

<%= item.big_story_id %>&nbsp;&nbsp;&nbsp;<a href="<%= item.url %>"><%= item.title %></a><br>
<% end %>
<% end %>
<% end %>